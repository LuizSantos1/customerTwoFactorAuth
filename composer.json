{
    "name": "magetarian/module-customer-two-factor-auth",
    "description": "2FA customer module for Magento 2",
    "homepage": "https://github.com/magetarian/customerTwoFactorAuth",
    "type": "magento2-module",
    "version": "1.0.0",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": "~7.2.0||~7.3.0",
        "magento/framework": "102.0.*",
        "magento/module-customer": "102.0.*",
        "magento/module-checkout": "100.3.*",
        "msp/twofactorauth": "3.1.*"
    },
    "license": [
        "GPL-3.0"
    ],
    "authors": [
        {
            "name": "Alexander Lukyanov",
            "email": "support@sashas.org"
        },
        {
            "name": "Atish Goswami",
            "email": "atishgoswami@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*",
        "magento/magento-coding-standard": "^5.0",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "~3.4.0"
